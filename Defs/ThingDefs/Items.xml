<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	
  <ThingDef ParentName="ResourceBase">
    <defName>EmptyCapsule</defName>
    <label>empty capsule</label>
    <description>A capsule that can contain cultured cells.</description>
    <graphicData>
      <texPath>Things/Item/finishcapsule</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>1</stackLimit>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <MarketValue>100</MarketValue>
      <Mass>1</Mass>
      <Flammability>0.2</Flammability>
      <DeteriorationRate>10</DeteriorationRate>
    </statBases>
    <intricate>true</intricate>
    <thingCategories>
      <li>BiotechnologyCategory_Main</li>
    </thingCategories>
  </ThingDef>

 <ThingDef Abstract="True" Name="BioCultureBase" ParentName="OrganicProductBase" >
    <graphicData>
      <texPath>Things/Item/finishcapsule</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <tickerType>Normal</tickerType>	
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <Flammability>0.2</Flammability>
      <DeteriorationRate>10</DeteriorationRate>
    </statBases>	
    <socialPropernessMatters>true</socialPropernessMatters>
    <comps>
      <li Class="CompProperties_TemperatureRuinable">
        <minSafeTemperature>-25</minSafeTemperature>
        <maxSafeTemperature>40</maxSafeTemperature>
        <progressPerDegreePerTick>0.00005</progressPerDegreePerTick>
      </li>
    </comps>	
    <stackLimit>1</stackLimit>		
  </ThingDef>

